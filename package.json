  "scripts": {
    "// ============================== CLI ============================== ": "",
    "7z": "7z",
    "cecho": "cecho",
    "clean": "rimraf www",
    "download": "download",
    "imagemin": "imagemin",
    "json": "json",
    "modernizr": "modernizr",
    "ncu": "ncu",
    "replace": "replace",
    "scrape": "scrape-it",
    "scrape:node": "npm run scrape -- -c ./scrape-node.json -u https://nodejs.org/en/download/current/",
    "scrape:nginx": "npm run scrape -- -c ./scrape-nginx.json -u http://nginx.org/en/download.html",
    "svg-sprite": "svg-sprite",
    "typings": "typings",
    "uglifyjs": "uglifyjs",
    "// ============================== BATCH JOBS ============================== ": "",
    "postinstall": "call install.bat",
    "update": "call update.bat",
    "pretest": "eslint script",
    "test": "karma start --single-run --browsers PhantomJS_custom",
    "prebuild": "cecho \"test & clean:\" && run-p -s test clean",
    "build": "cecho \"build:\" && run-p -s \"build:*\"",
    "build:css": "cecho -v e \"stylus assets/styles/main.styl -m -o dist/ && hashmark -s -l 8 -m assets.json dist/main.css 'dist/{name}{hash}{ext}'\"",
    "build:html": "cecho -v e \"jade assets/markup/index.jade --obj assets.json -o dist\"",
    "build:img": "cecho \"build:img\" && run-p -s build:img:*",
    "build:img:gif": "imagemin src/img/**/*.gif --out-dir=www/img --plugin=gifsicle",
    "build:img:jpg": "imagemin src/img/**/*.{jpg,jpeg} --out-dir=www/img --plugin=mozjpeg",
    "build:img:png": "imagemin src/img/**/*.png --out-dir=www/img --plugin=pngcrush",
    "build:img:svg": "imagemin src/img/**/*.svg --out-dir=www/img --plugin=svgo",
    "build:js": "cecho \"build:js\" && r_js -o build.js > logs/build.log",
    "build:js:mobile": "cecho \"build:js:mobile\" && r_js -o build.js uglify.screw_ie8=true > logs/build.log",
    "postbuild": "ncp src/js/libs www/js/libs \"--filter=(\\.css|(\\.|-)min\\.js)$|\\.(?!(js|map)$)([^.]+$)\" -s",
    "bundle": "call bundle.bat",
    "android": "cecho \"emulate:\" && cordova emulate android && adb remount && adb push Config/hosts /system/etc",
    "android:run": "cecho \"run:\" && cordova run android --device",
    "deploy": "run-s -s build android",
    "deploy:run": "run-s -s build android:run",
    "deploy:prod": "cecho -v e \"s3-cli sync ./www/ s3://nubrid-com/prod-site/\"",
    "deploy:stage": "cecho -v e \"s3-cli sync ./www/ s3://nubrid-com/stage-site/\"",
    "// ============================== SERVERS ============================== ": "",
    "karma": "karma start",
    "nginx": "cmd.exe /k \"cd Config\\nginx && nginx && cd ..\\..\"",
    "pm2": "pm2",
    "serve": "node ./bin/app -s false",
    "serve:dev": "SET NODE_ENV=development&& npm run -s serve",
    "serve:prod": "SET NODE_ENV=production&& npm run -s serve",
    "serve:nginx": "run-p -s nginx serve",
    "serve:pm2": "SET NODE_ENV=development&& pm2 start pm2.json && pm2 logs",
    "serve:json": "json-server --port 8400 --watch json.server.db.json",
    "start": "run-p -s nginx serve:pm2",
    "stop": "run-p stop:nginx stop:pm2",
    "stop:nginx": "cmd.exe /k \"cd Config\\nginx && nginx -s stop && cd ..\\..\"",
    "stop:pm2": "pm2 stop all && run-s stop:node",
    "stop:node": "cmd.exe /k \"taskkill /f /im node.exe\"",
    "testem": "testem"
  },
  "// ============================== ESLINT ============================== ": "",
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:lodash/recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended",
      "prettier/react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "ecmaFeatures": {
        "globalReturn": true
      },
      "sourceType": "module"
    },
    "plugins": [
      "immutable"
    ],
    "rules": {
      "immutable/no-let": 2,
      "immutable/no-this": 2,
      "no-var": 2,
      "no-param-reassign": 2,
      "lodash/prefer-lodash-method": [
        2,
        {
          "ignoreMethods": [
            "endsWith",
            "filter",
            "forEach",
            "startsWith",
            "trim"
          ]
        }
      ],
      "// NOTE: prettier override": "ADD: no-unexpected-multiline, REMOVE: indent, comma-style",
      "prettier/prettier": [
        2,
        {
          "semi": false,
          "trailingComma": "all"
        }
      ],
      "no-unexpected-multiline": 2,
      "linebreak-style": [
        2,
        "windows"
      ],
      "no-bitwise": 2,
      "eqeqeq": 2,
      "guard-for-in": 2,
      "no-extend-native": 2,
      "wrap-iife": 2,
      "no-use-before-define": 2,
      "new-cap": 2,
      "no-caller": 2,
      "no-new": 2,
      "no-plusplus": 2,
      "quotes": [
        2,
        "double"
      ],
      "strict": 0,
      "max-params": [
        2,
        5
      ],
      "max-depth": [
        2,
        5
      ],
      "max-statements": [
        2,
        25
      ],
      "complexity": [
        2,
        10
      ],
      "no-eval": 2,
      "block-scoped-var": 2,
      "no-iterator": 2,
      "no-loop-func": 2,
      "no-multi-str": 2,
      "no-proto": 2,
      "no-script-url": 2,
      "no-shadow": [
        2,
        {
          "hoist": "all",
          "allow": [
            "event"
          ]
        }
      ],
      "dot-notation": 2
    },
    "globals": {
      "__DEV__": false,
      "__MOBILE__": false,
      "__URL__": false,
      "AppManager": false,
      "Modernizr": false,
      "PropTypes": false,
      "React": false,
      "ReactDOM": false,
      "ReactRedux": false,
      "Redux": false,
      "require": false,
      "workbox": false,
      "chai": false,
      "sinon": false
    }
  },
  "eslintIgnore": []
}
