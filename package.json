  "scripts": {
    "7z": "7z",
    "babel": "babel --plugins transform-es2015-modules-amd",
    "cecho": "cecho",
    "clean": "rimraf www",
    "coffee": "coffee",
    "download": "download",
    "imagemin": "imagemin",
    "json": "json",
    "karma": "karma start",
    "modernizr": "modernizr",
    "nginx": "cmd.exe /k \"cd Config\\nginx && nginx && cd ..\\..\"",
    "replace": "replace",
    "sprity": "sprity",
    "svg-sprite": "svg-sprite",
    "testem": "testem",
    "typings": "typings",
    "uglifycss": "uglifycss",
    "uglifyjs": "uglifyjs",
    "webpack": "webpack",
    "postinstall": "call install.bat",
    "postupdate": "call update.bat",
    "pretest": "jshint script",
    "test": "karma start --single-run --browsers PhantomJS_custom",
    "prebuild": "cecho \"test & clean:\" && run-p -s test clean",
    "build": "cecho \"build:\" && run-p -s \"build:*\"",
    "build:css": "cecho -v e \"stylus assets/styles/main.styl -m -o dist/ && hashmark -s -l 8 -m assets.json dist/main.css 'dist/{name}{hash}{ext}'\"",
    "build:html": "cecho -v e \"jade assets/markup/index.jade --obj assets.json -o dist\"",
    "build:img": "cecho \"build:img\" && run-p -s build:img:*",
    "build:img:gif": "imagemin src/img/**/*.gif www/img -P gifsicle",
    "build:img:jpg": "imagemin src/img/**/*.{jpg,jpeg} www/img -P mozjpeg -p",
    "build:img:png": "imagemin src/img/**/*.png www/img -P pngcrush --reduce true",
    "build:img:svg": "imagemin src/img/**/*.svg www/img -P svgo",
    "build:js": "cecho \"build:js\" && r_js -o build.js > logs/build.log",
    "build:js:mobile": "cecho \"build:js:mobile\" && r_js -o build.js uglify.screw_ie8=true > logs/build.log",
    "postbuild": "ncp src/js/libs www/js/libs \"--filter=(\\.css|(\\.|-)min\\.js)$|\\.(?!(js|map)$)([^.]+$)\" -s",
    "bundle": "call bundle.bat",
    "debug": "node-debug -p 8500 -d 8501 ./bin/app -s false",
    "debug:babel": "bode-debug -p 8500 -d 8501 ./bin/app -s false",
    "android": "cecho \"emulate:\" && cordova emulate android && adb remount && adb push Config/hosts /system/etc",
    "android:run": "cecho \"run:\" && cordova run android --device",
    "deploy": "run-s -s build android",
    "deploy:dev": "run-s -s build \"ncp www/index.dev.html www/index.html\" android",
    "deploy:run": "run-s -s build android:run",
    "deploy:run:dev": "run-s -s build \"ncp www/index.dev.html www/index.html\" android:run",
    "deploy:run:home": "run-s -s build \"ncp www/index.home.html www/index.html\" android:run",
    "deploy:prod": "cecho -v e \"s3-cli sync ./www/ s3://nubrid-com/prod-site/\"",
    "deploy:stage": "cecho -v e \"s3-cli sync ./www/ s3://nubrid-com/stage-site/\"",
    "watch": "run-p -s watch:*",
    "watch:babel": "babel ./script --plugins babel-plugin-add-module-exports,transform-runtime --ignore ./script/js/libs --copy-files --watch --out-dir ./src",
    "watch:coffee": "coffee -cw ./test",
    "watch:livereload": "livereload src/ --port 8100",
    "pm2": "pm2",
    "serve": "node ./bin/app -s false",
    "serve:dev": "SET NODE_ENV=development&& npm run -s serve",
    "serve:prod": "SET NODE_ENV=production&& npm run -s serve",
    "serve:pm2": "SET NODE_ENV=development&& pm2 start pm2.json && pm2 logs",
    "serve:json": "json-server --port 8400 --watch json.server.db.json",
    "start": "run-p -s nginx serve:pm2 watch:*",
    "stop": "run-p stop:nginx stop:pm2",
    "stop:nginx": "cmd.exe /k \"cd Config\\nginx && nginx -s stop && cd ..\\..\"",
    "stop:pm2": "pm2 stop all && run-s stop:node",
    "stop:node": "cmd.exe /k \"taskkill /f /im node.exe\"",
    "open": "run-p -s open:explorer open:cmd open:cmd",
    "open:cmd": "cmd.exe /k \"start cmd.exe\"",
    "open:explorer": "cmd.exe /k \"start . && explorer .\""
  },
  "jshintConfig": {
    "bitwise": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "immed": true,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": false,
    "nonbsp": true,
    "nonew": true,
    "plusplus": false,
    "quotmark": "double",
    "undef": true,
    "unused": true,
    "strict": false,
    "maxparams": 5,
    "maxdepth": 5,
    "maxstatements": 25,
    "maxcomplexity": 10,
    "asi": false,
    "boss": false,
    "debug": false,
    "eqnull": false,
    "esversion": 6,
    "moz": false,
    "evil": false,
    "expr": false,
    "funcscope": false,
    "globalstrict": false,
    "iterator": false,
    "lastsemic": false,
    "laxbreak": true,
    "laxcomma": true,
    "loopfunc": false,
    "multistr": false,
    "noyield": false,
    "notypeof": false,
    "proto": false,
    "scripturl": false,
    "shadow": false,
    "sub": false,
    "supernew": false,
    "validthis": false,
    "browser": true,
    "devel": true,
    "jquery": true,
    "node": true,
    "globals": {
      "__DEV__": false,
      "__MOBILE__": false,
      "__URL__": false,
      "AppManager": false,
      "_": false,
      "Backbone": false,
      "BackboneImmutable": false,
      "BackbonePouch": false,
      "BackboneReactMixin": false,
      "Connection": false,
      "define": false,
      "Marionette": false,
      "Modernizr": false,
      "PouchDB": false,
      "Primus": false,
      "React": false,
      "ReactDOM": false,
      "ReactCSSTransitionGroup": false,
      "ReactPureRenderMixin": false,
      "ReactTransitionGroup": false,
      "require": false,
      "requirejs": false
    }
  },
  "babel": {
    "presets": [
      "react"
    ],
    "plugins": [
      "check-es2015-constants",
      "transform-es2015-arrow-functions",
      "transform-es2015-block-scoped-functions",
      "transform-es2015-block-scoping",
      "transform-es2015-classes",
      "transform-es2015-computed-properties",
      "transform-es2015-destructuring",
      "transform-es2015-duplicate-keys",
      "transform-es2015-for-of",
      "transform-es2015-literals",
      "transform-es2015-modules-commonjs",
      "transform-es2015-object-super",
      "transform-es2015-parameters",
      "transform-es2015-shorthand-properties",
      "transform-es2015-spread",
      "transform-es2015-sticky-regex",
      "transform-es2015-template-literals",
      "transform-es2015-typeof-symbol",
      "transform-es2015-unicode-regex",
      "transform-regenerator",
      "transform-strict-mode"
    ]
  },
