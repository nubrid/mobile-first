{
  "name": "Nubrid",
  "version": "0.3.0",
  "description": "Nubrid",
  "main": "Nubrid.js",
  "dependencies": {
    "engine.io": "1.3.x",
    "express": "3.x",
    "express-useragent": "0.1.x",
    "node-rest-client": "1.0.x",
    "node-static": "0.7.x",
    "passport": "0.2.x",
    "passport-facebook": "1.0.x",
    "passport-linkedin": "0.1.x",
    "passport-twitter": "1.0.x",
    "primus-broadcast": "1.1.x",
    "primus.io": "3.1.x",
    "requirejs": "2.1.x",
    "seed": "0.1.x"
  },
  "devDependencies": {
    "backbone": "^1.2.1",
    "backbone-react-component": "^0.8.1",
    "backbone.marionette": "^2.4.2",
    "chai": "^3.0.0",
    "chai-as-promised": "^5.1.0",
    "chai-jquery": "^2.0.0",
    "download-cli": "^1.0.1",
    "jquery": "^2.1.4",
    "jquery-mobile": "^1.4.1",
    "jshint": "latest",
    "karma": "^0.12.37",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coffee-preprocessor": "^0.2.1",
    "karma-firefox-launcher": "^0.1.6",
    "karma-ie-launcher": "^0.2.0",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-requirejs": "^0.2.2",
    "livereload": "^0.3.7",
    "lodash": "^3.10.0",
    "mocha": "^2.2.5",
    "ncp": "^2.0.0",
    "parallelshell": "^1.1.1",
    "phantomjs": "^1.9.17",
    "react": "^0.13.3",
    "replace": "^0.3.0",
    "requirejs": "^2.1.19",
    "rimraf": "^2.4.0",
    "sinon": "^1.15.4",
    "sinon-chai": "^2.8.0",
    "subschema": "^0.9.1",
    "testem": "^0.8.5",
    "uglify-js": "^2.4.23",
    "underscore": "^1.8.3"
  },
  "scripts": {
    "clean": "rimraf www",
    "download": "download",
    "lint": "jshint src",
    "replace": "replace",
    "karma": "karma start",
    "minify": "uglifyjs",
    "pretest": "npm run lint",
    "test": "npm run karma -- --single-run --browsers PhantomJS_custom",
    "prebuild": "npm run test && npm run clean -s",
    "build": "npm run build:js -s && npm run build:css -s",
    "build:js": "node node_modules/requirejs/bin/r.js -o src/js/main.build.js > build.log",
    "build:css": "echo \"stylus assets/styles/main.styl -m -o dist/ && hashmark -s -l 8 -m assets.json dist/main.css 'dist/{name}{hash}{ext}'\"",
    "build:html": "echo 'jade assets/markup/index.jade --obj assets.json -o dist'",
    "build:img": "echo 'copy img to www'",
    "build:libs": "npm update && call bundle.bat && ncp src/js/libs www/js/libs",
    "postbuild": "npm run build:libs",
    "deploy:prod": "echo 's3-cli sync ./www/ s3://nubrid-com/prod-site/'",
    "deploy:stage": "echo 's3-cli sync ./www/ s3://nubrid-com/stage-site/'",
    "deploy:dev": "npm run build -s && cordova emulate android && adb remount && adb push Config/hosts /system/etc",
    "watch": "parallelshell \"npm run watch:test -s\" \"npm run watch:build -s\"",
    "watch:test": "testem",
    "watch:build": "nodemon -q -w src/js -w src/css -w src/img --ext '.' --ignore src/js/main.build.js --exec 'npm run build'",
    "serve": "node Nubrid.js http httpstohttp",
    "livereload": "livereload src/ --port 8081",
    "start": "parallelshell \"npm run livereload -s\" \"npm run serve -s\" \"call Config/startup.nginx.bat\""
  },
  "jshintConfig": {
    "bitwise": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "immed": true,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonbsp": true,
    "nonew": true,
    "plusplus": true,
    "quotmark": "double",
    "undef": true,
    "unused": true,
    "strict": true,
    "maxparams": 5,
    "maxdepth": 5,
    "maxstatements": 25,
    "maxcomplexity": 10,
    "asi": false,
    "boss": false,
    "debug": false,
    "eqnull": false,
    "es5": false,
    "esnext": false,
    "moz": false,
    "evil": false,
    "expr": false,
    "funcscope": false,
    "globalstrict": false,
    "iterator": false,
    "lastsemic": false,
    "laxbreak": true,
    "laxcomma": true,
    "loopfunc": false,
    "multistr": false,
    "noyield": false,
    "notypeof": false,
    "proto": false,
    "scripturl": false,
    "shadow": false,
    "sub": false,
    "supernew": false,
    "validthis": false,
    "browser": true,
    "devel": true,
    "jquery": true,
    "globals": {
      "AppManager": false,
      "_": false,
      "Backbone": false,
      "Connection": false,
      "define": false,
      "Marionette": false,
      "Modernizr": false,
      "Primus": false,
      "React": false,
      "require": false
    }
  },
  "author": "Randell Rivera",
  "license": "ISC"
}
