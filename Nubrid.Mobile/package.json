{
  "name": "nubrid",
  "version": "1.0.0",
  "private": true,
  "description": "Nubrid",
  "main": "Nubrid.js",
  "dependencies": {
    "body-parser": "^1.13.2",
    "compression": "^1.5.1",
    "cookie-parser": "^1.3.5",
    "cookie-session": "^1.2.0",
    "engine.io": "^1.5.2",
    "express": "^4.13.1",
    "express-session": "^1.11.3",
    "express-useragent": "^0.1.9",
    "helmet": "^0.10.0",
    "jsonapi-serializer": "^1.0.4",
    "node-rest-client": "^1.5.1",
    "passport": "^0.2.2",
    "passport-facebook": "^2.0.0",
    "passport-linkedin": "^0.1.3",
    "passport-twitter": "^1.0.3",
    "primus-broadcast": "^1.1.0",
    "primus.io": "^3.2.0",
    "requirejs": "^2.1.19",
    "seed": "^0.4.4",
    "serve-static": "^1.10.0",
    "st": "^0.5.4",
    "yargs": "^3.15.0"
  },
  "devDependencies": {
    "backbone": "^1.2.1",
    "backbone-react-component": "^0.8.1",
    "backbone.marionette": "^2.4.2",
    "chai": "^3.0.0",
    "chai-as-promised": "^5.1.0",
    "chai-jquery": "^2.0.0",
    "download-cli": "^1.0.1",
    "faker": "^3.0.0",
    "jquery": "^2.1.4",
    "jquery-mobile": "^1.4.1",
    "jshint": "^2.8.0",
    "json-server": "^0.7.21",
    "karma": "^0.12.37",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coffee-preprocessor": "^0.2.1",
    "karma-firefox-launcher": "^0.1.6",
    "karma-ie-launcher": "^0.2.0",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-requirejs": "^0.2.2",
    "livereload": "^0.3.7",
    "lodash": "^3.10.0",
    "mocha": "^2.2.5",
    "ncp": "^2.0.0",
    "node-inspector": "^0.11.1",
    "parallelshell": "^1.1.1",
    "phantomjs": "^1.9.17",
    "react": "^0.13.3",
    "replace": "^0.3.0",
    "rimraf": "^2.4.0",
    "sinon": "^1.15.4",
    "sinon-chai": "^2.8.0",
    "subschema": "^0.9.1",
    "testem": "^0.8.5",
    "uglify-js": "^2.4.23",
    "underscore": "^1.8.3"
  },
  "scripts": {
    "cecho": "cecho",
    "clean": "rimraf www",
    "download": "download",
    "replace": "replace",
    "karma": "karma start",
    "minify": "uglifyjs",
    "testem": "testem",
    "pretest": "jshint src",
    "test": "karma start --single-run --browsers PhantomJS_custom",
    "prebuild": "cecho \"test & clean:\" && npm run test && npm run clean -s",
    "build": "cecho \"build:\" && npm run build:js -s && npm run build:css -s",
    "build:js": "node node_modules/requirejs/bin/r.js -o src/js/main.build.js > logs/build.log",
    "build:css": "cecho -v e \"stylus assets/styles/main.styl -m -o dist/ && hashmark -s -l 8 -m assets.json dist/main.css 'dist/{name}{hash}{ext}'\"",
    "build:html": "cecho -v e 'jade assets/markup/index.jade --obj assets.json -o dist'",
    "build:img": "cecho -v e 'copy img to www'",
    "build:libs": "cecho \"build libs:\" && call bundle.bat",
    "postbuild": "ncp src/js/libs www/js/libs",
    "debug": "node-debug -p 8500 -d 8501 ./bin/app -s false",
    "predeploy:dev": "npm run build -s",
    "deploy:prod": "cecho -v e 's3-cli sync ./www/ s3://nubrid-com/prod-site/'",
    "deploy:stage": "cecho -v e 's3-cli sync ./www/ s3://nubrid-com/stage-site/'",
    "deploy:dev": "cecho \"deploy:\" && cordova emulate android && adb remount && adb push Config/hosts /system/etc",
    "watch": "parallelshell \"npm run watch:test -s\" \"npm run watch:build -s\"",
    "watch:build": "nodemon -q -w src/js -w src/css -w src/img --ext '.' --ignore src/js/main.build.js --exec 'npm run build'",
    "serve": "node ./bin/app",
    "serve:prod": "SET NODE_ENV=production&& npm run serve -- -s false",
    "serve:dev": "SET NODE_ENV=development&& npm run serve -- -s false",
    "serve:json": "json-server --port 8400 --watch json.server.db.json",
    "livereload": "livereload src/ --port 8100",
    "start": "parallelshell \"npm run livereload -s\" \"npm run serve:prod -s\""
  },
  "jshintConfig": {
    "bitwise": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "immed": true,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonbsp": true,
    "nonew": true,
    "plusplus": true,
    "quotmark": "double",
    "undef": true,
    "unused": true,
    "strict": true,
    "maxparams": 5,
    "maxdepth": 5,
    "maxstatements": 25,
    "maxcomplexity": 10,
    "asi": false,
    "boss": false,
    "debug": false,
    "eqnull": false,
    "es5": false,
    "esnext": false,
    "moz": false,
    "evil": false,
    "expr": false,
    "funcscope": false,
    "globalstrict": false,
    "iterator": false,
    "lastsemic": false,
    "laxbreak": true,
    "laxcomma": true,
    "loopfunc": false,
    "multistr": false,
    "noyield": false,
    "notypeof": false,
    "proto": false,
    "scripturl": false,
    "shadow": false,
    "sub": false,
    "supernew": false,
    "validthis": false,
    "browser": true,
    "devel": true,
    "jquery": true,
    "globals": {
      "AppManager": false,
      "_": false,
      "Backbone": false,
      "Connection": false,
      "define": false,
      "Marionette": false,
      "Modernizr": false,
      "Primus": false,
      "React": false,
      "require": false
    }
  },
  "author": "Randell Rivera",
  "license": "ISC"
}
